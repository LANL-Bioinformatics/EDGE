					<tr><td>Reads</td><td><TMPL_VAR NAME=AFTERREADS DEFAULT=-> </td></tr>
					<tr><td>Total Bases</td><td><TMPL_VAR NAME=AFTERBASES DEFAULT=-> bp</td></tr>
					<tr><td>Mean Read Length</td><td><TMPL_VAR NAME=AFTERMRL DEFAULT=-> bp</td></tr>
					<tr><td>Paired Reads</td><td><TMPL_VAR NAME=AFTERPAIRED DEFAULT=-></td></tr>
					<tr><td>Paired Total Bases</td><td><TMPL_VAR NAME=AFTERPTB DEFAULT=-></td></tr>
					<tr><td>Unpaired Reads</td><td><TMPL_VAR NAME=AFTERUNPAIRED DEFAULT=-></td></tr>
					<tr><td>Unpaired Total Bases</td><td><TMPL_VAR NAME=AFTERUTB DEFAULT=-></td></tr>
				  </tbody>
				</table>
				<TMPL_IF NAME="OUT_IMAGE_READY">
				<TMPL_IF NAME="OUT_nanopore_QC_SW">
				<div class="ui-grid-b ui-responsive edge-report-plots">
					<div class="ui-block-a">
						<span class="iframe_label">Quality Report <a data-ajax='false' href='<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_length_quality.png'>[full]</a></span>
						<a href="#popup-general" data-rel="popup" data-position-to="window" data-transition="fade">
						<img class="preview_img" data-src="<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_length_quality.png" alt="QC_quality_report">
						</a>
					</div>
					<div class="ui-block-b">
						<span class="iframe_label">Read Length <a data-ajax='false' href='<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_read_length.png'>[full]</a></span>
						<a href="#popup-general" data-rel="popup" data-position-to="window" data-transition="fade"><img class="preview_img" data-src="<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_read_length.png" alt="QC_read_length"></a>
					</div>
					<div class="ui-block-c">
						<span class="iframe_label">Read Length (Log)<a data-ajax='false' href='<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_read_loglength.png'>[full]</a></span>
						<a href="#popup-general" data-rel="popup" data-position-to="window" data-transition="fade"><img class="preview_img" data-src="<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_read_loglength.png" alt="QC_read_log_length"></a>
					</div>
				</div>
				<TMPL_ELSE>
				<div class="ui-grid-c ui-responsive edge-report-plots">
					<div class="ui-block-a">
						<span class="iframe_label">Quality Report <a data-ajax='false' href='<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_quality_report.png'>[full]</a></span>
						<a href="#popup-general" data-rel="popup" data-position-to="window" data-transition="fade">
						<img class="preview_img" data-src="<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_quality_report.png" alt="QC_quality_report">
						</a>
					</div>
					<div class="ui-block-b">
						<span class="iframe_label">Read Length <a data-ajax='false' href='<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_read_length.png'>[full]</a></span>
						<a href="#popup-general" data-rel="popup" data-position-to="window" data-transition="fade"><img class="preview_img" data-src="<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_read_length.png" alt="QC_quality_report"></a>
					</div>
					<div class="ui-block-c">
						<span class="iframe_label">Nucleotide Cont <a data-ajax='false' href='<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_nucleotide_content.png'>[full]</a></span>
						<a href="#popup-general" data-rel="popup" data-position-to="window" data-transition="fade"><img class="preview_img" data-src="<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_nucleotide_content.png" alt="QC_quality_report"></a>
					</div>
					<div class="ui-block-d">
						<span class="iframe_label">Quality Boxplot <a data-ajax='false' href='<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_quality_boxplot.png'>[full]</a></span>
						<a href="#popup-general" data-rel="popup" data-position-to="window" data-transition="fade"><img class="preview_img" data-src="<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/HTML_Report/images/QC_quality_boxplot.png" alt="QC_quality_report"></a>
					</div>
				</div>
				</TMPL_IF>
				</TMPL_IF>
			</div>
			<p class='extra-text-link'>Link to
				|<a data-ajax='false' href='<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/QcReads/QC_qc_report.pdf'> QC Report PDF </a>
				|<a href='#edge-outputfile-dialog' class='edge-outputfile-tree' data-rel='popup' dir-src='<TMPL_VAR NAME=OUTPUTDIR DEFAULT=->/QcReads'> Directory </a>
			</p>
			</li>
			</TMPL_IF>
