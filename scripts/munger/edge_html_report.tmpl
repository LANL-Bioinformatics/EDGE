<!-- END sample metatdata -->	
		
		<TMPL_UNLESS NAME=OUT_CONLY_SW>
		<div data-role='collapsible' data-collapsed-icon='carat-d' data-expanded-icon='carat-u' data-collapsed='false' data-inset='false'>
			<TMPL_IF NAME="SRADERR">
			<p class="error"><TMPL_VAR NAME=SRADERR></p>
			</TMPL_IF>
			<TMPL_IF NAME="QCERR">
			<p class="error"><TMPL_VAR NAME=QCERR></p>
			</TMPL_IF>

			<h4>Pre-processing</h4>
			
			<ol>
			<li id="countTag"><span class="li-report-content-title">Raw Reads</span><div class="li-report-content">
				<table data-role='table' class='output-table output-table-twocol ui-responsive'>
				  <thead>
					<tr>
					  <th data-priority='1'>Raw Reads</th>
					  <th data-priority='2'>Stats</th>
					</tr>
				  </thead>
				  <tbody>
					<tr><td>Reads</td><td><TMPL_VAR NAME=BEFOREREADS DEFAULT=-></td></tr>
					<tr><td>Total Bases</td><td><TMPL_VAR NAME=BEFOREBASES DEFAULT=-> bp</td></tr>
					<tr><td>Mean Read Length</td><td><TMPL_VAR NAME=BEFOREMRL DEFAULT=-> bp</td></tr>
				  </tbody>
				</table>
			</div></li>
			<TMPL_IF NAME="OUT_QC_SW">
			<li id="qcTag" ><span class="li-report-content-title">Quality Trimming</span><div class="li-report-content">
